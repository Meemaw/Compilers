class Program{

    int i;
    real j;

    static void main(){
        int i,j;
        int sum;
        sum = 0;

        for(i = 0; i < 10; i++) {
            if(i == 5) {
                {
                    break;
                }
            }
        }
        writeln(i); // print 5


        for(j = 0; j < 10; j++) {
            if(j != 9) {
                {
                    continue;
                }
            }
            writeln(j); // prints 9
        }


        for(i = 0; i < 10; i++) {
            for(j = 0; j < 10; j++) {
